{
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "img/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
    "default",
    "custom_templates/fxstrap",
    "custom_templates/vvvv"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
	"globalMetadata": {
	  "_enableSearch": "true"
	},
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}